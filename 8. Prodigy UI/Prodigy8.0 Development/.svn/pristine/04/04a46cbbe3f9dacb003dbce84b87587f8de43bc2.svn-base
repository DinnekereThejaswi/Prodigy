        <div class="row">
    <div class="col-md-3">
        <p>{{SalesEstNo}}</p>
    </div>
   
    <div class="col-md-6">
        <pre>{{fieldArray | json}}</pre>
    </div>
</div> -->

<!-- Row -->
<ng-container *ngIf="EnableJson">
    <div class="col-md-3">
        <pre>{{PurchaseLinesPost | json}}</pre>
    </div>
</ng-container>

