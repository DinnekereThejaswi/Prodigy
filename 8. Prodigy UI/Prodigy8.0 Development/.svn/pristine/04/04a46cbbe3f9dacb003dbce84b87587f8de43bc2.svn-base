<ng-container *ngIf="ifPurchase">
    <div style="color: brown; text-align: right; font-style: italic;">
        <app-lastdocno [routerUrl]="routerUrl"></app-lastdocno>
    </div>
</ng-container>

<div class="row" *ngIf="ifPurchase">
    <div class="col-md-12">
        <label *ngFor="let radiobutton of radioItems" class="form-check-inline">
            <input type="radio" name="option" (click)="OnRadioBtnChnge(radiobutton)" class="form-check-input"
                [value]="radiobutton" [checked]="radiobutton === model.option">
            <h3 style="margin-top: 5px;">{{radiobutton}}</h3>
        </label>
    </div>
</div>

<div id="accordian" class="accordion" *ngIf="ifPurchase">
    <div class="card" *ngIf="ToggleEstimation">
        <div id="header" class="card-body show">
            <div class="row">
                <div class="col-md-3 ">
                    <div class="form-group">
                        <label for="exampleAccount">Est no</label>
                        <input type="text" class="form-control"
                            oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');"
                            onKeyPress="if(this.value.length==7) return false;" placeholder="Est No"
                            (change)="EstimationDetails(estNo.value)" GreenBorder #estNo>
                    </div>
                </div>
                <div class="col-md-3 ">
                    <div class="form-group">
                        <label for="barcode"></label>
                        <div class="input-group">
                            <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary"
                                style="margin-top: 5px;" (click)="EstimationDetails(estNo.value)"><i
                                    class="fal fa-search"></i> Search</button>
                        </div>
                    </div>
                </div>
                <!-- <div class="col-md-3">
                    <div class="card-body">
                        <div class="text-right"><i class="fal fa-calendar-alt"></i>&nbsp; <span class="text-muted">Order
                                Date</span>
                            <h1 class="font-light"></h1>
                        </div>
                    </div>
                </div> -->
            </div>
        </div>
    </div>
    <ng-container *ngIf="ShowHidePurEst">
        <br *ngIf="ToggleEstimation">
        <div class="card mb-0">
            <div class="card-header Pointer " (click)="ToggleCustomerData()" data-toggle="collapse" href="#Customer"
                [ngClass]="EnableCustomerTab ? 'collapsed' : ''" [attr.data-toggle]="EnableCustomerTab ? '':'collapse'"
                style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);">
                <div class="row">
                    <div class="col-md-4 ">
                        <a class="card-title align-middle " style="color:#0096FF">
                            <i class="fas fa-2x fa-users mr-2 align-middle"></i>
                            <span class="align-middle"><ins>Customer Details </ins>
                                <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                                    <span class="tooltiptext tooltip-top">Capture the New customer
                                        information and Existing customer information</span>
                                </div>
                            </span>

                        </a>
                    </div>
                    <ng-container *ngIf="CustomerDets">
                        <div class="col-md-4 pull-right">
                            <h4><span class="align-middle"> Customer: {{CustomerDets.CustName}} </span></h4>
                        </div>
                        <div class="col-md-4 pull-right">
                            <h4><span class="align-middle"> MobileNo: {{CustomerDets.MobileNo}} </span></h4>
                        </div>
                    </ng-container>
                </div>
            </div>
            <div id="Customer" class="card-body collapse show" [hidden]="EnableCustomerTab">
                <app-customer></app-customer>
            </div>
        </div>

    </ng-container>
</div>
<br>

<ng-container *ngIf="ShowHidePurEst">
    <div class="card">
        <div class="card-header Pointer"
            style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black"
            *ngIf="ifPurchase">
            <div class="row">
                <div class="col-md-2 ">
                    <a class="card-title align-middle Pointer" style="color:#FF00C5">
                        <i class="far fa-shopping-cart fa-2x mr-2 align-middle"></i>
                        <span class="align-middle Pointer"><ins>Purchase</ins></span>
                    </a>
                </div>
            </div>
        </div>
        <div class="card-body">
            <form [formGroup]="PurchaseForm" class="form-horizontal" (ngSubmit)="onSubmit()">
                <div class="row">
                    <div class="col-md-2 ">
                        <div class="form-group">
                            <label for="SalCode">Sales Person</label>
                            <div class="form-group">
                                <ng-container *ngIf="EnableDisablectrls">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="SalCode"
                                        [(ngModel)]="PurSalCode">
                                        <option [ngValue]="null" [disabled]="EnableDropdown">Select</option>
                                        <option *ngFor="let _salesman of SalesManList" [value]="_salesman.sal_code"
                                            [disabled]="EnableDropdown">
                                            {{_salesman.CodeName}}
                                        </option>
                                    </select>
                                </ng-container>
                                <ng-container *ngIf="!EnableDisablectrls">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="SalCode"
                                        [(ngModel)]="PurSalCode">
                                        <option [ngValue]="null" [disabled]="true">Select</option>
                                        <option *ngFor="let _salesman of SalesManList" [value]="_salesman.sal_code"
                                            [disabled]="true">
                                            {{_salesman.CodeName}}
                                        </option>
                                    </select>
                                </ng-container>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-2 ">
                        <div class="form-group">
                            <label for="GS">GS Type</label>
                            <div class="form-group">
                                <ng-container *ngIf="EnableDisablectrls">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" #GS formControlName="GS"
                                        (change)="getItemList(PurchaseForm)">
                                        <option [ngValue]="null" [disabled]="EnableDropdown">Select</option>
                                        <option *ngFor="let _GSList of GSList" [value]="_GSList.gs_code"
                                            [disabled]="EnableDropdown">
                                            {{_GSList.item_level1_name}}
                                        </option>
                                    </select>
                                </ng-container>
                                <ng-container *ngIf="!EnableDisablectrls">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" #GS formControlName="GS"
                                        (change)="getItemList(PurchaseForm)">
                                        <option [ngValue]="null" [disabled]="true">Select</option>
                                        <option *ngFor="let _GSList of GSList" [value]="_GSList.gs_code"
                                            [disabled]="true">
                                            {{_GSList.item_level1_name}}
                                        </option>
                                    </select>
                                </ng-container>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-2 ">
                        <div class="form-group">
                            <label for="RateType">Rate Type</label>
                            <div class="input-group">
                                <ng-container *ngIf="EnableDisablectrls">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" #RateType
                                        formControlName="RateType" (change)="toggleRateType(PurchaseForm)">
                                        <option *ngFor="let _RateTypeData of RateTypeData"
                                            [ngValue]="_RateTypeData.value" [disabled]="EnableRateType">
                                            {{_RateTypeData.name}}
                                        </option>
                                    </select>
                                </ng-container>
                                <ng-container *ngIf="!EnableDisablectrls">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" #RateType
                                        formControlName="RateType" (change)="toggleRateType(PurchaseForm)">
                                        <option *ngFor="let _RateTypeData of RateTypeData"
                                            [ngValue]="_RateTypeData.value" [disabled]="true">
                                            {{_RateTypeData.name}}
                                        </option>
                                    </select>
                                </ng-container>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-1 " *ngIf="EnableDisablectrls">
                        <div class="form-group float-left" style="margin-top: 2px;">
                            <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary pull-right"
                                style="margin-top: 1.2rem !important;" [disabled]="EnableAddRow"
                                (click)="add(PurchaseForm)">
                                <i class="far fa-plus-circle"></i> Add</button>
                        </div>
                    </div>
                    <div class="col-md-3 float-right">
                        <div class="form-group">
                            <div class="text-right"><i class="fal fa-rupee-sign"></i>&nbsp; <span
                                    class="text-muted">Date</span>
                                <h1 class="font-light">{{applicationDate | date:'dd/MM/yyyy'}}</h1>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-2 float-right">
                        <div class="form-group">
                            <div class="text-right"><i class="fal fa-rupee-sign"></i>&nbsp; <span
                                    class="text-muted">Rate/Gram</span>
                                <h1 class="font-light">{{Rate | number : '1.2-2'}}</h1>
                                <!-- <div class="form-group">
                    <input style="width: 80px;" class="form-control" id="RatePerGram" formControlName="RatePerGram" [(ngModel)]="RatePerGram"
                        placeholder="RatePerGram"type="number" [ngModelOptions]="{standalone: true}"  (change)=" RateperGram()">
                </div> -->
                            </div>
                        </div>
                    </div>
                </div>
            </form>

            <div class="table-responsive">
                <table class="table table-bordered">
                    <thead class="topbar">
                        <tr>
                            <th></th>
                            <th>Item</th>
                            <th>Gr. Wt</th>
                            <th>St. Wt</th>
                            <th>DM. Ct</th>
                            <th>Nt.Wt</th>
                            <th>Purity %</th>
                            <th>Melting %</th>
                            <th>M. Loss</th>
                            <th>Purchase Rate</th>
                            <th>Di/St.Amt</th>
                            <th>Amount</th>
                            <th *ngIf="EnableDisablectrls">Action</th>
                        </tr>
                    </thead>
                    <tbody *ngFor="let field of fieldArray; let i = index">
                        <tr>
                            <td>
                                <h3><a href="javascript:void(0)" (click)="sendDataToDiamondComp(field,i)"
                                        data-toggle="collapse" [attr.data-target]="'#collapse'+ i"
                                        id='panels'>Diamond</a>
                                    / <a href="javascript:void(0)" (click)="sendDataToStoneComp(field,i)"
                                        data-toggle="collapse" [attr.data-target]="'#collapseStone'+ i"
                                        id='panels'>Stone</a>
                                </h3>
                            </td>
                            <td>
                                <select class="form-control mr-auto mt-2 mt-lg-0 CustomWidth200" #Item
                                    name="{{field.ItemName}}" (change)="AJAXRateperGram(Item.value,i);"
                                    [(ngModel)]="field.ItemName">
                                    <option [ngValue]="null" [disabled]="readonly[i]">Select</option>
                                    <!-- <option *ngFor="let _ItemList of ItemList" [value]="_ItemList.Karat"
                                    [disabled]="readonly[i]">
                                    {{_ItemList.Item}}
                                </option> -->
                                    <option *ngFor="let _ItemList of ItemList" [value]="_ItemList.Item_code"
                                        [disabled]="readonly[i]">
                                        {{_ItemList.Item}}
                                    </option>
                                </select>
                            </td>
                            <td>
                                <input [(ngModel)]="field.GrossWt" [readonly]="readonly[i]"
                                    class="form-control CustomWidth50" type="text" (change)="GetNetWT(i);"
                                    appNumbersOnly min="0" name="{{field.GrossWt}}" placeholder="0.000" BlueBorder />
                            </td>
                            <td>
                                <ng-container
                                    *ngIf="ItemType != 'OGCN24K' && ItemType != 'OGCN' && ItemType != 'OG24K'">
                                    <input [(ngModel)]="field.StneWt" [readonly]="readonly[i]"
                                        class="form-control CustomWidth50" type="text" min="0"
                                        [ngModelOptions]="{standalone: true}" name="{{field.StneWt}}"
                                        (change)="GetNetWT(i);" BlueBorder placeholder="0.000" appNumbersOnly />
                                </ng-container>
                                <ng-container
                                    *ngIf="ItemType == 'OGCN24K' || ItemType == 'OGCN' || ItemType == 'OG24K'">
                                    <h3 [(ngModel)]="field.StneWt" ngDefaultControl
                                        [ngModelOptions]="{standalone: true}" class="align-middle">
                                        {{field.StneWt || "0.00" | number}}</h3>
                                </ng-container>
                            </td>
                            <ng-container
                                *ngIf="ItemType == 'OSSL' || ItemType == 'SGDE' || ItemType == 'SLOL' || ItemType == 'OG24K' || ItemType == 'OGCN' || ItemType == 'OGCN24K'">
                                <td>
                                    <input [(ngModel)]="field.Dcts" class="form-control CustomWidth50" type="text"
                                        min="0" [ngModelOptions]="{standalone: true}" name="{{field.Dcts}}"
                                        placeholder="0.000" readonly />
                                </td>
                            </ng-container>
                            <ng-container
                                *ngIf="ItemType != 'OSSL' && ItemType != 'SGDE' && ItemType != 'SLOL' && ItemType != 'OG24K' && ItemType != 'OGCN' && ItemType != 'OGCN24K'">
                                <td>
                                    <input [(ngModel)]="field.Dcts" class="form-control CustomWidth50" type="text"
                                        min="0" [readonly]="readonly[i]" (change)="chngDmdCrt(i)"
                                        [ngModelOptions]="{standalone: true}" BlueBorder name="{{field.Dcts}}"
                                        placeholder="0.000" appNumbersOnly />
                                </td>
                            </ng-container>
                            <td>
                                <h3 [(ngModel)]="field.NetWt" ngDefaultControl [ngModelOptions]="{standalone: true}"
                                    class="align-middle">{{field.NetWt || "0.00" | number}}</h3>
                            </td>
                            <td>
                                <ng-Container *ngIf="EnableDisablePurPer">
                                    <h3 [(ngModel)]="field.PurityPercent" ngDefaultControl
                                        [ngModelOptions]="{standalone: true}" class="align-middle">
                                        {{field.PurityPercent || "0.00" | number}}</h3>
                                </ng-Container>
                                <ng-Container *ngIf="!EnableDisablePurPer">
                                    <input [(ngModel)]="field.PurityPercent" [readonly]="readonly[i]"
                                        class="form-control CustomWidth50" type="text" min="0"
                                        [ngModelOptions]="{standalone: true}" name="{{field.PurityPercent}}" BlueBorder
                                        (change)="GetMLLoss_MLPercentage(i);" placeholder="0" appNumbersOnly />
                                </ng-Container>
                            </td>
                            <td>
                                <!-- <input [(ngModel)]="field.MeltingPercent" class="form-control" readonlytype="number" readonly
                        [ngModelOptions]="{standalone: true}" name="{{field.MeltingPercent}}" placeholder="0.00" /> -->

                                <h3 [(ngModel)]="field.MeltingPercent" ngDefaultControl
                                    [ngModelOptions]="{standalone: true}" class="align-middle">
                                    {{field.MeltingPercent || "0.00" | number}}</h3>
                            </td>
                            <td>
                                <!-- <input [(ngModel)]="field.MeltingLoss" class="form-control" type="number" readonly readonly
                        [ngModelOptions]="{standalone: true}" name="{{field.MeltingLoss}}" placeholder="0.00" /> -->

                                <h3 [(ngModel)]="field.MeltingLoss" ngDefaultControl
                                    [ngModelOptions]="{standalone: true}" class="align-middle">
                                    {{field.MeltingLoss || "0.000" | number}}</h3>
                            </td>
                            <td>
                                <ng-container
                                    *ngIf="ItemType != 'OGPP' && ItemType != 'SGDE' && ItemType != 'SLOL' && ItemType != 'OSSL' && ItemType != 'OG22' && EnablePurchaseRate">
                                    <input [(ngModel)]="field.PurchaseRate" class="form-control CustomWidth100"
                                        type="text" [readonly]="readonly[i]" [ngModelOptions]="{standalone: true}"
                                        name="{{field.PurchaseRate}}" appNumbersOnly placeholder="0.00"
                                        (change)="ItemAmountCalc(i);" BlueBorder />
                                </ng-container>
                                <ng-container
                                    *ngIf="ItemType == 'OGPP' || ItemType == 'OSSL' || ItemType == 'SGDE' || ItemType == 'SLOL'">
                                    <input [(ngModel)]="field.PurchaseRate" class="form-control CustomWidth100"
                                        type="text" [readonly]="readonly[i]" [ngModelOptions]="{standalone: true}"
                                        name="{{field.PurchaseRate}}" appNumbersOnly placeholder="0.00"
                                        (change)="ItemAmountCalc(i);" BlueBorder />

                                </ng-container>
                                <ng-container
                                    *ngIf="ItemType != 'OGPP' && ItemType != 'OSSL' && ItemType != 'SGDE' && ItemType != 'SLOL' && ItemType != 'OG22' && !EnablePurchaseRate">
                                    <h3 [(ngModel)]="field.PurchaseRate" ngDefaultControl
                                        [ngModelOptions]="{standalone: true}" class="align-middle">
                                        {{field.PurchaseRate || "0.00" | number}}</h3>
                                </ng-container>
                                <ng-container *ngIf="ItemType == 'OG22'">
                                    <h3 [(ngModel)]="field.PurchaseRate" ngDefaultControl
                                        [ngModelOptions]="{standalone: true}" class="align-middle">
                                        {{field.PurchaseRate || "0.00" | number}}</h3>
                                </ng-container>
                            </td>
                            <td>
                                <!-- <input [(ngModel)]="field.DiamondAmount" class="form-control" type="number" readonly
                        [ngModelOptions]="{standalone: true}" name="{{field.DiamondAmount}}" placeholder="0.00" /> -->
                                <h3 [(ngModel)]="field.DiamondAmount" ngDefaultControl
                                    [ngModelOptions]="{standalone: true}" class="align-middle">
                                    {{field.DiamondAmount || "0.00" | number}}</h3>
                            </td>
                            <td>
                                <!-- <input [(ngModel)]="field.ItemAmount" class="form-control" type="number" readonly
                        [ngModelOptions]="{standalone: true}" name="{{field.ItemAmount}}" placeholder="0.00" /> -->
                                <h3 [(ngModel)]="field.ItemAmount" ngDefaultControl
                                    [ngModelOptions]="{standalone: true}" class="align-middle">
                                    {{field.ItemAmount || "0.00" | number}}</h3>
                            </td>
                            <td colspan="2" *ngIf="EnableDisablectrls">
                                <ng-container *ngIf="EnableEditDelbtn[i]">
                                    <button class="btn btn-default btn-rounded" type="button"
                                        (click)="editFieldValue(i)"><i class="fal fa-pencil-alt"></i>
                                        Edit</button>
                                    <button class="btn btn-danger btn-rounded" type="button"
                                        (click)="deleteFieldValue(i)"><i class="fal fa-trash-alt"></i>
                                        Delete</button>
                                </ng-container>
                                <ng-container *ngIf="EnableSaveCnlbtn[i]">
                                    <button class="btn btn-success btn-rounded" type="button"
                                        (click)="saveDataFieldValue(i)"><i class="fal fa-save"></i>
                                        Save</button>
                                    <button class="btn btn-warning btn-rounded" type="button"
                                        (click)="cancelDataFieldValue(i)"><i class="far fa-window-close"></i>
                                        Cancel</button>
                                </ng-container>
                            </td>
                        </tr>
                        <tr>
                            <td colspan="13" class="hiddenRow" *ngIf="i === Index">
                                <div class="collapse wrapper" id="collapse{{i}}" data-parent="#panels">
                                    <app-diamond (diamondAmount)="receiveStoneDiamondAmt()"
                                        (EnableDmdBtn)="EnableDisableDmdBtn($event)"
                                        [PurchaseLinesDetails]="LinesDetails" [EnableDiamondTab]="EnableDiamondTab"
                                        [EstNo]="SalesEstNo" [DmCarats]="DmCarats"></app-diamond>
                                </div>
                                <div class="collapse wrapper" id="collapseStone{{i}}" data-parent="#panels">
                                    <app-stone (stoneAmount)="receiveStoneDiamondAmt()"
                                        (EnableStnBtn)="EnableDisableStnBtn($event)"
                                        [PurchaseLinesDetails]="LinesDetails" [EnableStoneTab]="EnableStoneTab"
                                        [EstNo]="SalesEstNo" [StoneCarats]="StoneCarats"></app-stone>
                                </div>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <br>
            <!-- Row -->
            <div class="row">
                <!-- Column -->
                <div class="col-lg-4 col-md-6">
                    <div class="card">
                        <div class="row">
                            <div class="col-12">
                                <div class="social-widget">
                                    <div class="soc-content">
                                        <div class="col-6 b-r">
                                            <h3 class="font-medium">{{GrossWtTotal(fieldArray) || "0.00" | number}}</h3>
                                            <h5 class="text-muted">Total Gr. Wt.</h5>
                                        </div>
                                        <div class="col-6">
                                            <h3 class="font-medium">{{StneWt(fieldArray) || "0.000" | number}}</h3>
                                            <h5 class="text-muted">Total St. Wt.</h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Column -->
                <!-- Column -->
                <div class="col-lg-4 col-md-6">
                    <div class="card">
                        <div class="row">
                            <div class="col-12">
                                <div class="social-widget">
                                    <div class="soc-content">
                                        <div class="col-6 b-r">
                                            <h3 class="font-medium">{{NetWt(fieldArray) || "0.00" | number}}</h3>
                                            <h5 class="text-muted">Total Net Wt.</h5>
                                        </div>
                                        <div class="col-6">
                                            <h3 class="font-medium">{{Dcts(fieldArray) || "0.00" | number}}</h3>
                                            <h5 class="text-muted">Total Carats</h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Column -->
                <!-- Column -->
                <div class="col-lg-4 col-md-6">
                    <div class="card">
                        <div class="row">
                            <div class="col-12">
                                <div class="social-widget">
                                    <div class="soc-content">
                                        <div class="col-4 b-r">
                                            <h3 class="font-medium">{{Amount(fieldArray) | number: '1.2-2'}}</h3>
                                            <h5 class="text-muted">Item Total</h5>
                                        </div>
                                        <div class="col-4 b-r">
                                            <h3 class="font-medium">{{rndoff | number: '1.2-2'}}</h3>
                                            <h5 class="text-muted">Round Off</h5>
                                        </div>
                                        <div class="col-4 b-r">
                                            <h3 class="font-medium">{{Amount(fieldArray) | currencyFormat}}</h3>
                                            <h5 class="text-muted">Est.Total</h5>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <br>
            <ng-container *ngIf="EnableDisablectrls">
                <div class="row pt-1">
                    <div class="col-md-4">
                        <button
                            class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
                            (click)="SavePurchaseEst()">
                            <i class="far fa-save"></i> Submit</button>
                        <button class="btn btn-danger waves-effect waves-light btn-rounded btn-secondary" type="button"
                            (click)="cancelPurchaseEst()"><i class="far fa-window-close"></i>&nbsp;Cancel</button>
                    </div>
                </div>
                <br>
            </ng-container>
            <ng-container *ngIf="EnableReprint">
                <app-reprint-purchaseestimation></app-reprint-purchaseestimation>
            </ng-container>
        </div>
    </div>
</ng-container>


<span *ngIf="ifPurchase" style="color: red;">Note : All weights are in grams and Amount in INR</span>



