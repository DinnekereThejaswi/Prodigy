<form [formGroup]="AWBPrintForm">
    <div class="row">
        <div class="col-md-12">
            <div class="form-group CustomWidth">
                <label for="OrderNo">Order No</label>
                <div class="input-group">
                    <input type="text" class="form-control" formControlName="OrderNo" (change)="getAwb(AWBPrintForm)" />
                </div>
            </div>
            <div class="form-group CustomWidth">
                <label for="AWBNo">AWB No</label>
                <div class="input-group">
                    <input type="text" class="form-control" formControlName="AWBNo" readonly />
                </div>
            </div>
            <div class="form-group">
                <button class="printMe btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                    (click)="printShipLabelContent(AWBPrintForm)"><i class="fal fa-print"></i>
                    Print </button>
                <button class="btn btn-danger waves-effect waves-light btn-rounded btn-secondary" type="button"
                    (click)="clearAWB()"><i class="far fa fa-trash"></i>&nbsp;Clear</button>
            </div>
        </div>
    </div>
</form>