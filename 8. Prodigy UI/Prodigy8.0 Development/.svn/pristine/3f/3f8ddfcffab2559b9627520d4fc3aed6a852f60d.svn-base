<div class="modal fade" id="ReprintTypeModal" tabindex="-1" role="dialog" aria-labelledby="ReprintTypeTabLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="container invoice-box htmlPrint" style="max-width: 500px;border-width: 2em">
                <div class="invoice">
                    <h3 style="text-align:center"><b>Reprint Type</b></h3>
                    <div class="card">
                        <div class="card-body">
                            <div class="form-group">
                                <div class="col-md-12">
                                    <label *ngFor="let radiobuttonReprintType of radioReprintTypeItems"
                                        class="form-check-inline">
                                        <input type="radio" name="option"
                                            (click)="ChangedReprintType(radiobuttonReprintType)"
                                            class="form-check-input" [value]="radiobuttonReprintType"
                                            [checked]="radiobuttonReprintType === modelReprintType.option">
                                        <h3 style="margin-top: 10px;">{{radiobuttonReprintType}}</h3>
                                    </label>
                                </div>
                            </div>
                            <div class="form-group" style="text-align: center;">
                                <button
                                    class="printMe btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                                    (click)="onReprint(reprintHeaders.selectedOption)"><i class="fal fa-print"></i>
                                    Print</button>
                                <button
                                    class="printMe btn btn-danger waves-effect waves-light btn-rounded btn-secondary"
                                    (click)="CanelPrint()"><i class="fal fa-times"></i>
                                    Cancel</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>



