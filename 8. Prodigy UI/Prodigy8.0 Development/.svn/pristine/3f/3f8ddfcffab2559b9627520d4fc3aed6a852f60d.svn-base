<div class="row" *ngIf="isReprint" style="display:block;margin: auto;width: 30%;">
    <form [formGroup]="ReprintForm" class="form-horizontal">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <ul class="list-group">
                                    <li class="list-group-item">
                                        <!-- <div class="col-md-12"> -->
                                        <label for="OrderNo">Date</label>&nbsp;
                                        <ng-container *ngIf="!EnableDate">
                                            <input type="text" class="form-control" bsDatepicker
                                                [bsConfig]="datePickerConfig" [(ngModel)]="applicationDate"
                                                formControlName="applicationDate" readonly
                                                (ngModelChange)="byDate(applicationDate)"
                                                value="{{applicationDate | date:'dd/MM/yyyy' }}"
                                                style="width: 200px;" />
                                        </ng-container>
                                        <ng-container *ngIf="EnableDate">
                                            <input type="text" class="form-control" [(ngModel)]="applicationDate"
                                                formControlName="applicationDate"
                                                value="{{applicationDate | date:'dd/MM/yyyy' }}" style="width: 200px;"
                                                readonly />
                                        </ng-container>
                                        &nbsp;&nbsp;
                                        <i class="fal fa-edit" (click)="ValidatePermisiion()"></i>
                                        <!-- </div> -->
                                    </li>
                                </ul>
                            </div>
                            <div class="form-group">
                                <div class="input-group" *ngFor="let radiobutton of radioItems">
                                    <ul class="list-group">
                                        <div class="col-md-12">
                                            <label class="form-check-inline">
                                                <input type="radio" class="form-check-input" [value]="radiobutton"
                                                    (change)="Changed(radiobutton)"
                                                    [checked]="radiobutton === model.option">
                                                <h3 style="margin-top: 5px;">{{radiobutton}}</h3>
                                            </label>
                                        </div>
                                    </ul>
                                </div>
                            </div>
                            <ng-container *ngIf="EnableCanOrders">
                                <div class="form-group">
                                    <ul class="list-group">
                                        <li class="list-group-item">
                                            Cancelled Orders
                                            <div class="material-switch pull-right">
                                                <input id="someSwitchOptionDefault" (change)="isChecked($event)"
                                                    name="someSwitchOption001" type="checkbox" />
                                                <label for="someSwitchOptionDefault" class="label-default"></label>
                                            </div>
                                        </li>
                                    </ul>

                                </div>
                            </ng-container>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group" *ngIf="toggleOrderNo">
                                <div class="input-group">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="selectedOption"
                                        [(ngModel)]="reprintHeaders.selectedOption">
                                        <option [ngValue]="null">Select Order No</option>
                                        <option *ngFor="let _OrderNoList of OrderNoList" [value]="_OrderNoList.OrderNo">
                                            {{_OrderNoList.OrderNo}} - {{_OrderNoList.CustName}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group" *ngIf="toggleReceiptNo">
                                <div class="input-group">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="selectedOption"
                                        [(ngModel)]="reprintHeaders.selectedOption">
                                        <option [ngValue]="null">Select Receipt No</option>
                                        <option *ngFor="let _ReceiptNoList of ReceiptNoList"
                                            [value]="_ReceiptNoList.ReceiptNo">
                                            {{_ReceiptNoList.ReceiptNo}} - {{_ReceiptNoList.CustomerName}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group" *ngIf="toggleClosedOrders">
                                <div class="input-group">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="selectedOption"
                                        [(ngModel)]="reprintHeaders.selectedOption">
                                        <option [ngValue]="null">Select Closed Orders</option>
                                        <option *ngFor="let _ClosedOrderList of ClosedOrderList"
                                            [value]="_ClosedOrderList.OrderNo">
                                            {{_ClosedOrderList.OrderNo}} - {{_ClosedOrderList.CustName}}
                                        </option>
                                    </select>
                                </div>
                            </div>
                            <!-- <div class="form-group" *ngIf="toggleOrderNo && toggleChecked">
                                    <div class="input-group">
                                        <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="selectedOption"
                                            [(ngModel)]="reprintHeaders.selectedOption">
                                            <option [ngValue]="null">Select Order No</option>
                                            <option *ngFor="let _CancelledOrderList of CancelledOrderList" [value]="_CancelledOrderList.OrderNo">
                                                {{_CancelledOrderList.OrderNo}}
                                            </option>
                                        </select>
                                    </div>
                                </div>
                                <div class="form-group" *ngIf="toggleReceiptNo && toggleChecked">
                                        <div class="input-group">
                                            <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="selectedOption"
                                                [(ngModel)]="reprintHeaders.selectedOption">
                                                <option [ngValue]="null">Select Receipt No</option>
                                                <option *ngFor="let _CancelledReceiptList of CancelledReceiptList"
                                                    [value]="_CancelledReceiptList.ReceiptNo">
                                                    {{_CancelledReceiptList.ReceiptNo}}
                                                </option>
                                            </select>
                                        </div>
                                    </div> -->

                            <!-- <div class="form-group">
                                <div class="input-group" *ngFor="let radioPrintType of radioItemPrintType">
                                    <ul class="list-group">
                                        <div class="col-md-12">
                                            <label class="form-check-inline">
                                                <input type="radio" class="form-check-input" [value]="radioPrintType"
                                                    (change)="ChangedPrintType(radioPrintType)"
                                                    [checked]="radioPrintType === modelPrintType.option">
                                                <h3 style="margin-top: 5px;">{{radioPrintType}}</h3>
                                            </label>
                                        </div>
                                    </ul>
                                </div>
                            </div> -->

                            <div class="form-group">
                                <button
                                    class="printMe btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                                    (click)="onPrint(reprintHeaders.selectedOption)"><i class="fal fa-print"></i>
                                    Reprint </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </form>
</div>

