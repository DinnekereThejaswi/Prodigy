<div class="row" style="display:block;margin: auto;width: 30%;">
    <form class="form-horizontal" [formGroup]="CancelOnlineOrdersForm">
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label for="">Market Place</label>
                                <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="marketplaceID"
                                    (change)="OnlineOrders(CancelOnlineOrdersForm)">
                                    <option [value]="null">Select</option>
                                    <option *ngFor="let _marketPlaceList of marketPlaceList"
                                        [value]="_marketPlaceList.Id">
                                        {{_marketPlaceList.Name}}
                                    </option>
                                </select>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <div class="input-group">
                                    <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="orderNo">
                                        <option [value]="null">Select Order No</option>
                                        <option *ngFor="let _onlineOrdersList of OnlineOrdersList"
                                            [value]="_onlineOrdersList.Code">{{_onlineOrdersList.Code}}</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="form-group">
                                <label class="input-group">
                                    <input type="text" class="form-control" placeholder="Enter Remarks"
                                        formControlName="cancelRemarks">
                                </label>
                            </div>
                        </div>
                    </div>

                    <div class="form-group">
                        <button class="printMe btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                            (click)="cancelOnlineOrder(CancelOnlineOrdersForm)"><i class="fas fa-ban"></i>
                            Cancel </button>
                        <button class="printMe btn btn-danger waves-effect waves-light btn-rounded btn-secondary"
                            (click)="clear()"><i class="fal fa-trash"></i>
                            Clear </button>
                    </div>

                </div>
            </div>
        </div>
    </form>
</div>

<ngx-spinner bdColor="rgba(0, 0, 0, 0.8)" size="medium" color="#fff" type="square-jelly-box" [fullScreen]="true">
    <p style="color: white"> Please Wait... </p>
</ngx-spinner>