<div class="row">
    <div class="col-lg-12">
        <div class="form-body">
            <div class="row">
                <div class="col-md-2">
                    <div class="form-group">
                        <label class="control-label">Mark Barcode No<span style="color: red;">&nbsp;*</span></label>
                        <div class="m-b-10">
                            <input type="text"
                                oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');">
                        </div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="control-label">Barcode No</label>
                        <div class="m-b-10">
                            <input type="text"
                                oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');">
                        </div>
                    </div>
                </div>
                <div class="col-md-6 ">
                </div>
            </div>
        </div>
    </div>
</div>
<form class="form-horizontal">
    <div class="row">
        <div class="col-lg-12">
            <div class="form-body">
                <fieldset>
                    <legend></legend>
                    <div class="row">
                        <div class="col-6">
                            <div class="row p-t-20">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <div class="m-b-10">
                                            <fieldset>
                                                <legend>Display Online</legend>
                                                <label *ngFor="let checkbutton of checkItems"
                                                    class="custom-control custom-radio align-middle"
                                                    style="display: inline-block;padding-right: 0.5rem;">
                                                    <input type="checkbox" name="option" class="form-check-input"
                                                        [checked]="checkbutton === model.option" [value]="checkbutton">
                                                    <h4 style="margin-top: 1px;">{{checkbutton}}</h4>
                                                </label>
                                            </fieldset>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <label class="control-label">GS</label>
                                        <select class="form-control mr-auto mt-2 mt-lg-0">
                                            <option [ngValue]="null">Select</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                </div>
                                <div class="col-md-6">
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="row p-t-20">
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <div class="form-group">
                                            <label class="control-label">Item Name</label>
                                            <select class="form-control mr-auto mt-2 mt-lg-0">
                                                <option [ngValue]="null">Select</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                    <div class="form-group">
                                        <fieldset>
                                            <legend>Status</legend>
                                            <div class="m-b-10">
                                                <label *ngFor="let radiobutton of radioItems"
                                                    class="custom-control custom-radio align-middle"
                                                    style="display: inline-block;padding-right: 0.5rem;">
                                                    <input type="radio" name="option" class="form-check-input"
                                                        [checked]="radiobutton === model.option" [value]="radiobutton">
                                                    <h4 style="margin-top: 1px;">{{radiobutton}}</h4>
                                                </label>
                                            </div>
                                        </fieldset>
                                    </div>
                                </div>
                                <div class="col-md-6">
                                </div>
                                <div class="col-md-4">
                                </div>
                                <div class="col-md-2">
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="row p-t-20">
                                <div class="col-md-6">

                                </div>
                                <div class="col-md-6">

                                </div>
                                <div class="col-md-6">

                                </div>
                                <div class="col-md-4">
                                </div>
                                <div class="col-md-2">
                                    <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary"><i
                                            class="fal fa-search"></i> Search</button>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br>
                </fieldset>
            </div>
        </div>
    </div>
</form>

<div class="table-responsive">
    <table class="table" id="panels">
        <thead class="topbar">
            <tr>
                <th>Barcode No</th>
                <th>GS Code</th>
                <th>Item Name</th>
                <th>Quantity</th>
                <th>Gross Wt</th>
                <th>Net Wt</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>46342519</td>
                <td>NGO</td>
                <td>BGBB</td>
                <td>1</td>
                <td>12.540</td>
                <td>12.300</td>
            </tr>
        </tbody>
    </table>
</div>
<br>