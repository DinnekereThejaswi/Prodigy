<div class="card">
  <div class="card-body">
    <form class="form-horizontal">
      <div class="row">
        <div class="col-md-2">
          <div class="form-group">
            <label for="">Tag No:</label>
            <!-- <input type="number" class="form-control" name="TagNo" #TagNo (keyup.enter)="getTagDetails(TagNo.value)"> -->
            <input class="form-control" name="TagNo" type="text" (keyup.enter)="getTagDetails(TagNo.value)"
              onkeypress='return (event.charCode > 96 && event.charCode < 123) || (event.charCode > 64 && event.charCode < 91) || (event.charCode >= 48 && event.charCode <= 57) || event.charCode <= 31'
              autocomplete="off" autofocus placeholder="Please Enter Tag No" #TagNo GreenBorder>
          </div>
        </div>
        <ng-container *ngIf="EnableTag1Details">
          <div class="col-md-2">
            <div><span class="text-muted">
                Gr.Wt:</span>
              <h2 class="font-light">{{GrWt | number : '1.3-3'}}</h2>
            </div>
          </div>
          <div class="col-md-2">
            <!-- <div>
              <span class="text-muted">
                Supplier:</span>
              <select class="form-control mr-auto mt-2 mt-lg-0" [(ngModel)]="Tag1DetailsJson[0].SupplierCode"
                [ngModelOptions]="{standalone: true}">
                <option value="null" [disabled]="true">Select</option>
                <option [disabled]="true" *ngFor="let _supplierList of SupplierList"
                  [value]="_supplierList.SupplierCode">
                  {{_supplierList.SupplierName}}
                </option>
              </select>
            </div> -->
            <div><span class="text-muted">
                St.Wt:</span>
              <h2 class="font-light">{{Tag1DetailsJson[0].Swt | number : '1.3-3'}}</h2>
            </div>
          </div>
          <div class="col-md-2">
            <!-- <div><span class="text-muted">
                Item:</span>
              <select class="form-control mr-auto mt-2 mt-lg-0" [(ngModel)]="HeaderItemName"
                [ngModelOptions]="{standalone: true}">
                <option value="null" [disabled]="true">Select</option>
                <option [disabled]="true" *ngFor="let _itemList of ItemList" [value]="_itemList.Item_code">
                  {{_itemList.Item}}
                </option>
              </select>
            </div> -->
            <div><span class="text-muted">
                Net.Wt:</span>
              <h2 class="font-light">{{NtWt | number : '1.3-3'}}</h2>
            </div>
          </div>
          <div class="col-md-2">
            <div><span class="text-muted">
                Pur MC Value:</span>
              <h2 class="font-light">{{Tag1DetailsJson[0].PurMcGram | number : '1.2-2'}}</h2>
            </div>
          </div>
          <div class="col-md-2">
            <div><span class="text-muted">
                Pur MC Amt:</span>
              <h2 class="font-light">{{Tag1DetailsJson[0].TotalPurchaseMc | number : '1.2-2'}}</h2>
            </div>

          </div>
        </ng-container>
      </div>

      <div class="row">
        <ng-container *ngIf="EnableTag1Details">
          <div class="col-md-3">
            <!-- <div><span class="text-muted">
                St.Wt:</span>
              <h2 class="font-light">{{Tag1DetailsJson[0].Swt | number : '1.3-3'}}</h2>
            </div> -->
            <div>
              <span class="text-muted">
                Supplier:</span>
              <select class="form-control mr-auto mt-2 mt-lg-0" [(ngModel)]="Tag1DetailsJson[0].SupplierCode"
                [ngModelOptions]="{standalone: true}">
                <option value="null" [disabled]="true">Select</option>
                <option [disabled]="true" *ngFor="let _supplierList of SupplierList"
                  [value]="_supplierList.SupplierCode">
                  {{_supplierList.SupplierName}}
                </option>
              </select>
            </div>
          </div>
          <div class="col-md-3">
            <div><span class="text-muted">
                Pur MC Type:</span>
              <select class="form-control mr-auto mt-2 mt-lg-0" [(ngModel)]="HeaderMcType"
                [ngModelOptions]="{standalone: true}">
                <option value="null" [disabled]="true">Select</option>
                <option [disabled]="true" *ngFor="let _mcTypeList of MCTypeList" [value]="_mcTypeList.MC_ID">
                  {{_mcTypeList.MC_NAME}}
                </option>
              </select>
            </div>
          </div>
          <div class="col-md-3">
            <div><span class="text-muted">
                Design:</span>
              <select class="form-control mr-auto mt-2 mt-lg-0  " [(ngModel)]="HeaderDesignName"
                [ngModelOptions]="{standalone: true}">
                <option value="null" [disabled]="true">Select</option>
                <option [disabled]="true" *ngFor="let _designList of DesignList" [value]="_designList.Code">
                  {{_designList.Name}}
                </option>
              </select>
            </div>
          </div>
          <div class="col-md-3">
            <!-- <div><span class="text-muted">
                Net.Wt:</span>
              <h2 class="font-light">{{NtWt | number : '1.3-3'}}</h2>
            </div> -->

            <div><span class="text-muted">
                Item:</span>
              <select class="form-control mr-auto mt-2 mt-lg-0" [(ngModel)]="HeaderItemName"
                [ngModelOptions]="{standalone: true}">
                <option value="null" [disabled]="true">Select</option>
                <option [disabled]="true" *ngFor="let _itemList of ItemList" [value]="_itemList.Item_code">
                  {{_itemList.Item}}
                </option>
              </select>
            </div>
          </div>
        </ng-container>
      </div>
      <br>
    </form>
  </div>
</div>

<ng-container *ngIf="EnableTag1Details">
  <div id="accordian" class="accordion">
    <div class="card mb-0">
      <div class="card-header Pointer" href="#Tag1Dets" [ngClass]="EnableTag1DetsTab ? 'collapsed' : ''"
        (click)="ToggleTag1Dets()" style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);"
        [attr.data-toggle]="EnableTag1DetsTab ? '':'collapse'">
        <div class="row">
          <div class="col-md-2 ">
            <a class="card-title align-middle" style="color:#005BFF">
              <i class="fas fa-tags fa-2x mr-2 align-middle"></i>
              <span class="align-middle Pointer"><ins>Tag 1 Details</ins></span>
            </a>
          </div>
        </div>
      </div>
      <div id="Tag1Dets" class="card-body collapse" [hidden]="EnableTag1DetsTab"
        [ngClass]="EnableTag1DetsTab ? 'hide' : 'show'">
        <div class="table-responsive">
          <table class="table table-bordered" style="width: 2000px;">
            <thead class="topbar">
              <tr>
                <th></th>
                <th>Item</th>
                <th>Design</th>
                <th>Qty</th>
                <th>Karat</th>
                <th>Gr.Wt</th>
                <th>St.Wt</th>
                <th>Nt.Wt</th>
                <th>Counter</th>
                <th>Size</th>
                <th>MC Type</th>
                <th>MC/grm</th>
                <th>MC/Piece</th>
                <th>VA%</th>
                <th>Wast%</th>
                <th>Wast grams</th>
                <th>Piece Rate</th>
                <th>St.Amt</th>
                <th>Dm.Amt</th>

                <th>Pur Rate</th>
                <th>Pur Type</th>
                <th>Pur Mc</th>
                <th>Pur St.Amt</th>
                <th>Pur Dm.Amt</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <td>
                  <h3><a href="javascript:void(0)" (click)="Tag1StoneDets(i)" data-toggle="collapse"
                      [attr.data-target]="'#collapse'+ i" id='panels'>Stone Details</a>
                  </h3>
                </td>
                <td style="width:200px;">
                  <select class="form-control mr-auto mt-2 mt-lg-0 CustomWidth200"
                    [(ngModel)]="Tag1DetailsJson[0].ItemName" [ngModelOptions]="{standalone: true}">
                    <option value="null">Select</option>
                    <option *ngFor="let _itemList of ItemList" [value]="_itemList.Item_code">
                      {{_itemList.Item}}
                    </option>
                  </select>
                </td>
                <td style="width:200px;">
                  <select class="form-control mr-auto mt-2 mt-lg-0 CustomWidth200"
                    [(ngModel)]="Tag1DetailsJson[0].DesignNo" [ngModelOptions]="{standalone: true}">
                    <option value="null">Select</option>
                    <option *ngFor="let _designList of DesignList" [value]="_designList.Code">
                      {{_designList.Name}}
                    </option>
                  </select>
                </td>
                <td>
                  <input class="form-control CustomWidth50" type="number" [(ngModel)]="Tag1DetailsJson[0].Qty" min="0"
                    placeholder="0.00" />
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].Karat}}</h3>
                </td>
                <td>
                  <input class="form-control CustomWidth100" type="number" [(ngModel)]="Tag1DetailsJson[0].Gwt" min="0"
                    placeholder="0.00" (change)="GetSplitBarcodeDetails()" />
                </td>
                <td>
                  <input class="form-control CustomWidth100" type="number" [(ngModel)]="Tag1DetailsJson[0].Swt" min="0"
                    placeholder="0.00" (change)="GetSplitBarcodeDetails()" />
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].Nwt | number : '1.3-3'}}</h3>
                </td>
                <td>
                  <select class="form-control mr-auto mt-2 mt-lg-0 CustomWidth100"
                    [(ngModel)]="Tag1DetailsJson[0].CounterCode" [ngModelOptions]="{standalone: true}">
                    <option value="null" [disabled]="true">Select</option>
                    <option [disabled]="true" *ngFor="let _counterList of CounterList"
                      [value]="_counterList.CounterCode">
                      {{_counterList.CounterName}}
                    </option>
                  </select>
                </td>
                <td>
                  <select class="form-control mr-auto mt-2 mt-lg-0 CustomWidth100"
                    [(ngModel)]="Tag1DetailsJson[0].ItemSize" [ngModelOptions]="{standalone: true}">
                    <option value="null" [disabled]="true">Select</option>
                    <option [disabled]="true" *ngFor="let _sizeList of SizeList" [value]="_sizeList.Code">
                      {{_sizeList.Name}}
                    </option>
                  </select>
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].McType}}</h3>
                </td>
                <td>
                  <input class="form-control CustomWidth50" type="number"
                    [(ngModel)]="Tag1DetailsJson[0].MakingChargePerRs" min="0" placeholder="0.00" />
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].McPerPiece}}</h3>
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].McPercent}}</h3>
                </td>
                <td>
                  <input class="form-control CustomWidth100" type="number" [(ngModel)]="Tag1DetailsJson[0].WastePercent"
                    min="0" placeholder="0.00" />
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].WastageGrams | number : '1.3-3'}}</h3>
                </td>
                <td *ngIf="Tag1DetailsJson[0].PieceRate == 0">
                  <h3 class="align-middle">{{Tag1DetailsJson[0].PieceRate | number : '1.2-2'}}</h3>
                </td>
                <td *ngIf="Tag1DetailsJson[0].PieceRate > 0">
                  <input class="form-control CustomWidth100" type="number" [(ngModel)]="Tag1DetailsJson[0].PieceRate" min="0"
                    placeholder="0.00" value="{{Tag1DetailsJson[0].PieceRate | number : '1.2-2'}" />
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].StoneAmount | number : '1.2-2'}}</h3>
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].DiamondAmount | number : '1.2-2'}}</h3>
                </td>

                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].PurRate | number : '1.2-2'}}</h3>
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].PurMcType | number : '1.2-2'}}</h3>
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].PurMcGram | number : '1.2-2'}}</h3>
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].PurStoneAmount | number : '1.2-2'}}</h3>
                </td>
                <td>
                  <h3 class="align-middle">{{Tag1DetailsJson[0].PurDiamondAmount | number : '1.2-2'}}</h3>
                </td>

              </tr>
              <br>
              <tr>
                <td colspan="20" class="hiddenRow" *ngIf="i === Tag1Index">
                  <fieldset class="scheduler-border">
                    <legend class="scheduler-border"><i class="fas fa-gem align-middle"></i> Stone Details</legend>
                    <div class="wrapper">
                      <form [formGroup]="Stone1Form" class="form-horizontal">
                        <div class="row">
                          <div class="col-md-1">
                            <div class="form-group">
                              <label for="">GS</label>
                              <div class="form-group">
                                <select class="form-control mr-auto mt-2 mt-lg-0 " #GS
                                  (change)="getGScodeTag1(GS.value)">
                                  <option value="null">Select</option>
                                  <option *ngFor="let _stoneGsTypeList of StoneGsTypeList"
                                    [value]="_stoneGsTypeList.Code">
                                    {{_stoneGsTypeList.Name}}
                                  </option>
                                </select>
                              </div>
                            </div>
                          </div>
                          <div class="col-md-2 ">
                            <div class="form-group">
                              <label for="">Type</label>
                              <div class="form-group">
                                <select class="form-control mr-auto mt-2 mt-lg-0" #Type
                                  (change)="getTypeTag1(Type.value)">
                                  <option value="null">Select</option>
                                  <option value="Ordinary">Ordinary</option>
                                  <option value="Precious">Precious</option>
                                  <option value="Semi Precious">Semi Precious</option>
                                </select>
                              </div>
                            </div>
                          </div>
                          <div class="col-md-2">
                            <div class="form-group">
                              <label for="Type">Name</label>
                              <div class="form-group">
                                <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="Type">
                                  <option value="null">Select</option>
                                  <option *ngFor="let _stoneNameAPI of StoneNameAPI" [value]="_stoneNameAPI.Code">
                                    {{_stoneNameAPI.Name}}
                                  </option>
                                </select>
                              </div>
                            </div>
                          </div>
                          <div class="col-md-1">
                            <div class="form-group">
                              <label for="Qty">Qty</label>
                              <div class="form-group">
                                <input class="form-control" type="number" formControlName="Qty" placeholder="0.00" />
                              </div>
                            </div>
                          </div>
                          <div class="col-md-1">
                            <div class="form-group">
                              <label for="Carrat">Carat</label>
                              <div class="form-group">
                                <input class="form-control" formControlName="Carrat" type="number" placeholder="0.00" />
                              </div>
                            </div>
                          </div>
                          <div class="col-md-1">
                            <div class="form-group">
                              <label for="Rate">Selling Rate</label>
                              <div class="form-group">
                                <input class="form-control" formControlName="Rate" type="number" placeholder="0.00" />
                              </div>
                            </div>
                          </div>
                          <div class="col-md-2">
                            <div class="form-group float-left" style="margin-top: 2px;">
                              <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary pull-right"
                                style="margin-top: 1.2rem !important;" (click)="add(Stone1Form)">
                                <i class="far fa-plus-circle"></i> Add</button>
                            </div>
                            <div class="form-group float-left" style="margin-top: 2px;">
                              <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary pull-right"
                                style="margin-top: 1.2rem !important;" (click)="cancel(Stone1Form)">
                                <i class="fa fa-times-circle"></i> Cancel</button>
                            </div>
                          </div>
                        </div>
                      </form>

                      <div>
                        <table class="table table-bordered" style="width: 1500px;">
                          <thead class="topbar">
                            <tr>
                              <th>Action</th>
                              <th>SNo</th>
                              <th>Name</th>
                              <th>Qty</th>
                              <th>Carat</th>
                              <th>Selling Amount</th>
                            </tr>
                          </thead>
                          <tbody *ngFor="let Tag1StoneDets of Tag1DetailsJson[0].lstOfStone; let i=index">
                            <tr>
                              <td>
                                <button class="btn btn-danger btn-rounded" type="button"
                                  (click)="deleteStone1FieldValue(i,Tag1StoneDets)"><i class="fal fa-trash-alt"></i>
                                  Delete</button>
                              </td>
                              <td style="width:200px;">
                                <h3 class="align-middle">{{i+1}}</h3>
                              </td>
                              <td>
                                <h3 class="align-middle">{{Tag1StoneDets.Name}}</h3>
                              </td>
                              <td>
                                <h3 class="align-middle">{{Tag1StoneDets.Qty}}</h3>
                              </td>
                              <td>
                                <h3 class="align-middle">{{Tag1StoneDets.Carrat | number : '1.3-3'}}</h3>
                              </td>
                              <td>
                                <h3 class="align-middle">{{Tag1StoneDets.Amount | number : '1.2-2'}}</h3>
                              </td>
                            </tr>
                          </tbody>
                        </table>
                      </div>
                    </div>
                  </fieldset>
                </td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</ng-container>
<br>

