<div class="card">
  <div class="card-body">
    <div class="row">
      <form [formGroup]="TDSMasterForm" class="form-horizontal" autocomplete="off">
        <div class="row">
          <div class="col-md-2">
            <div class="form-group">
              <label class="control-label" for="TDSName">TDSName <span class="textRed"> <i
                    class="fas fa-star-of-life"></i></span></label>
              <input oninput="this.value = this.value.toUpperCase()" type="text" class="form-control" id="TDSName"
                formControlName="frmCtrl_TDSName" style="text-transform: uppercase;"
                (input)="Codes.value = Codes.value.toUpperCase()" placeholder="Please Enter TDS Name"
                [(ngModel)]="TdsListData.TDSName" #Codes>
            </div>
          </div>
          <div class="col-md-2">
            <div class="form-group">
              <label class="control-label" for="TDS">TDS(%)<span class="textRed"> <i
                    class="fas fa-star-of-life"></i></span></label>
              <input type="number" class="form-control" id="TDS" formControlName="frmCtrl_TDS"
                placeholder="Please Enter TDS %" [(ngModel)]="TdsListData.TDS">
            </div>
          </div>
          <div class="col-md-2">
            <div class="form-group">
              <label class="control-label" for="TDSName">SubGroup <span class="textRed"> <i
                    class="fas fa-star-of-life"></i></span></label>
                    <select  class="form-control" id="TDS" formControlName="frmCtrl_TDS"
                    [(ngModel)]="TdsListData.TDS">
                    <option>select</option>
                    <option></option>
                   </select>
             
            </div>
          </div>
          <div class="col-md-2">
            <div class="form-group">
              <label class="control-label" for="TDS">A/C Ledger<span class="textRed"> <i
                    class="fas fa-star-of-life"></i></span></label>
              <select  class="form-control" id="TDS" formControlName="frmCtrl_TDS"
                 [(ngModel)]="TdsListData.TDS">
                 <option>select</option>
                 <option></option>
                </select>
            </div>
          </div>
          <div class="col-sm-3">
            <ng-container *ngIf="EnableAdd">
              <button class="btn btn-success waves-effect waves-light btn-rounded btn-secondary MT-2"
                style="margin-top: 1.2rem !important;" (click)="Add(TDSMasterForm)">
                <i class="far fa-plus-circle"></i>&nbsp;Add</button>
            </ng-container>
            <ng-container *ngIf="EnableSave">
              <button class="btn btn-success waves-effect waves-light btn-rounded btn-secondary MT-2"
                style="margin-top: 1.2rem !important;" (click)="save(TDSMasterForm)">
                <i class="fal fa-save"></i>&nbsp;Save</button>
            </ng-container>
            <button class="btn  btn-warning waves-effect waves-light btn-rounded btn-secondary MT-2"
              style="margin-top: 1.2rem !important;" (click)="clear()">
              <i class="fal fa-trash"></i>&nbsp;Clear</button>
          </div>
          <div class="col-sm-1">
            <br>
            <button class="btn btn-success waves-effect waves-light btn-rounded btn-secondary MT-2" (click)="Print()"
              type="submit"><i class="fal fa-print"></i> Print</button>
          </div>
        </div>
      </form>

    </div>
  </div>
</div>

<br>
<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-body">
        <div class="row">
          <div class="col-md-5">
            <h3> TDS Master</h3>
          </div>
        </div>
        <h6 class="card-subtitle"></h6>
        <div class="table-responsive">
          <table class="table table-bordered">
            <thead class="topbar">
              <tr>
                <!-- <th>ID</th>-->

                <th>TDS Name</th>
                <th>TDS %</th>
                <th>Status</th>
                <th>Edit</th>
                <th>Open</th>
                <th>Close</th>
              </tr>
            </thead>

            <tbody *ngFor="let List of TDSRecordList">
              <!-- <tr *ngFor="let kar of Location | search: searchText | paginate: { itemsPerPage:top, totalItems:MainCount, currentPage: p }" [ngClass]="{'btn-primary': cust.ID == currentId }"> -->
              <tr>
                <!-- <td>{{currency.ID}}</td>-->
                <td>{{List.TDSName}}</td>
                <td>{{List.TDS}}</td>
                <td [style.color]="getStatusColor(List.ObjectStatus)">
                  <b>{{ List.ObjectStatus | activeDeactive :List.ObjectStatus }}</b>
                </td>
                <!-- <td [style.color]="getStatusColor(List.ObjectStatus)"><b>{{List.ObjectStatus}}</b></td> -->
                <td>
                  <i class="fas fa-edit" title="edit" (click)="edit(List)"></i>
                </td>
                <td>
                  <i class="fas fa-lock-open" title="open" (click)="Activate(List)"></i>
                </td>
                <td>
                  <i class="fas fa-lock" title="close" (click)="Deactivate(List)"></i>
                </td>

              </tr>
            </tbody>
          </table>
          <ng-container *ngIf="EnableJson">
            <pre>{{TdsListData | json}}</pre>
          </ng-container>
        </div>
      </div>
    </div>
  </div>
</div>

<div class="modal fade" id="PrintDetailsTab" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog modal-lg" role="document" style="max-width: 900px">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary"
          id="exampleModalLabel" (click)=print()><i class="fal fa-print"></i>
          Print</button>
        <h3 class="modal-title"> TDS REPORT</h3>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <div class="row">
          <div class="col-md-12">
            <div class="table-responsive" id="TableData">
              <table class="table table-bordered table-hover">
                <thead class="topbar">
                  <tr>
                    <th>TDS Name</th>
                    <th>TDS %</th>
                    <th>Status</th>
                  </tr>
                </thead>
                <tbody *ngFor="let List of printData;let i=index">
                  <tr>
                    <td>{{List.TDSName}}</td>
                    <td>{{List.TDS}}</td>
                    <td>{{List.ObjectStatus}}</td>
                  </tr>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>