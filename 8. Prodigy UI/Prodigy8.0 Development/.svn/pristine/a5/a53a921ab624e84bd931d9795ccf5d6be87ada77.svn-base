<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-body">
        <form [formGroup]="MachineForm" class="form-horizontal">
          <div class="row">
            <div class="col-md-3">
              <div class="form-group">

                <label class="control-label" for="MachineName">Machine <span class="textRed"> <i
                      class="fas fa-star-of-life"></i></span></label>
                <select class="form-control" formControlName="MachineName" [(ngModel)]="MachineListData.MachineName">
                  <option [ngValue]="null">Select</option>
                  <option *ngFor="let _MachineList of MachineList" [ngValue]="_MachineList.ComputerName">
                    {{_MachineList.ComputerName}}
                  </option>
                </select>
              </div>
            </div>
            <div class="col-md-3">
              <div class="form-group">
                <label class="control-label" for="BillCounter">Counter<span class="textRed"> <i
                      class="fas fa-star-of-life"></i></span>
                </label>
                <select class="form-control" formControlName="BillCounter" [(ngModel)]="MachineListData.BillCounter">
                  <option [ngValue]="null">Select</option>
                  <option *ngFor="let _List of BillCounterList" [ngValue]="_List.BillCode">
                    {{_List.BillCode}}
                  </option>
                </select>
              </div>
            </div>

          </div>
          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <button type="submit" class="btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                  (click)="onSubmit(MachineForm)"><i class="fal fa-save"></i> Submit</button>
                <button type="submit"
                  class="btn btn-danger  waves-effect waves-light btn-rounded btn-secondary">Cancel</button>
              </div>
            </div>
            <div class="col-md-6" style="text-align:right">
              <span class="textRed"> <i class="fas fa-star-of-life"></i></span> Mandatory fields
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
  <pre>{{ MachineListData | json }}</pre>
</div>
