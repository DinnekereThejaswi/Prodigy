<div class="row">
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <form [formGroup]="SubModuleform" class="form-horizontal">
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label class="control-label" for="ModuleName">Module Name <span class="textRed"> <i
                                            class="fas fa-star-of-life"></i></span>
                                    <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                                        <span class="tooltiptext tooltip-top">Select Module Name </span>
                                    </div>
                                </label>
                                <select class="form-control mr-auto mt-2 mt-lg-0" formControlName="ModuleID"
                                    [(ngModel)]="postSubMainModule.ModuleID" (change)="getSubModuleByID()">
                                    <option [ngValue]="null">Select</option>
                                    <option *ngFor="let _moduleList of ModuleList" [value]="_moduleList.ID">
                                        {{_moduleList.Name}}
                                    </option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label class="control-label" for="SubModuleName">Sub Module Name <span class="textRed">
                                        <i class="fas fa-star-of-life"></i></span>
                                    <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                                        <span class="tooltiptext tooltip-top">Sub Module Name</span>
                                    </div>
                                </label>
                                <input type="text" class="form-control" id="Name" placeholder="Enter Sub Module Name"
                                    formControlName="Name" [(ngModel)]="postSubMainModule.Name" AlphaNumericValidator />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label class="control-label" for="SubID">Sub ID <span class="textRed"> <i
                                            class="fas fa-star-of-life"></i></span>
                                    <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                                        <span class="tooltiptext tooltip-top">Sub ID </span>
                                    </div>
                                </label>
                                <input type="number" class="form-control" id="SequenceNo" placeholder="Enter Sub ID"
                                    formControlName="ID" [(ngModel)]="postSubMainModule.ID" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label class="control-label" for="ModuleSeqNo">Module Seq.No <span class="textRed"> <i
                                            class="fas fa-star-of-life"></i></span>
                                    <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                                        <span class="tooltiptext tooltip-top">Module Sequence No </span>
                                    </div>
                                </label>
                                <input type="number" class="form-control" id="SequenceNo"
                                    placeholder="Enter Module Sequence No" formControlName="SortOrder"
                                    [(ngModel)]="postSubMainModule.SortOrder" />
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-3">
                            <div class="form-group">
                                <label class="control-label" for="UIRoute">UIRoute<span class="textRed"></span>
                                    <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                                        <span class="tooltiptext tooltip-top">Menu UIRoute </span>
                                    </div>
                                </label>
                                <input type="text" class="form-control" id="UIRoute" placeholder="Enter UIRoute"
                                    formControlName="UIRoute" [(ngModel)]="postSubMainModule.UIRoute" />
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label class="control-label" for="Icon">Icon<span class="textRed"></span>
                                    <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                                        <span class="tooltiptext tooltip-top">Menu Icon</span>
                                    </div>
                                </label>
                                <input type="text" class="form-control" id="Icon" placeholder="Enter Icon"
                                    [(ngModel)]="postSubMainModule.Icon" formControlName="Icon" />
                            </div>
                        </div>
                        <div class="col-md-3" style="padding-top:22px">
                            <div class="form-group">
                                <ng-container *ngIf="EnableAdd">
                                    <button type="submit"
                                        class="btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                                        (click)="Add()"><i class="fal fa-save"></i>&nbsp; Add</button>
                                </ng-container>
                                <ng-container *ngIf="EnableUpdate">
                                    <button type="submit"
                                        class="btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                                        (click)="Update()"><i class="fal fa-save"></i>&nbsp; Update</button>
                                </ng-container>
                                <button type="submit"
                                    class="btn btn-danger  waves-effect waves-light btn-rounded btn-secondary"
                                    (click)="Clear()"><i class="fal fa-remove"></i>&nbsp;Clear</button>
                            </div>
                        </div>
                        <div class="col-sm-1">
                            <br>
                            <button class="btn btn-success waves-effect waves-light btn-rounded btn-secondary MT-2"
                                (click)="Print()" type="submit"><i class="fal fa-print"></i> Print</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <div class="col-md-12">
        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table table-bordered">
                        <thead class="topbar">
                            <tr>
                                <th>Module ID</th>
                                <th>Module Name</th>
                                <th>Sub Module ID</th>
                                <th>Sub Module Name</th>
                                <th>Sequence</th>
                                <th>Status</th>
                                <th>Edit</th>
                                <th>Open</th>
                                <th>Close</th>

                            </tr>
                        </thead>
                        <tbody *ngFor="let _subModuleList of SubModuleList;let i=index;">
                            <tr>
                                <td>{{_subModuleList.ModuleID}}</td>
                                <td>{{_subModuleList.Name}}</td>
                                <td>{{_subModuleList.ID}}</td>
                                <td>{{_subModuleList.Name}}</td>
                                <td>{{_subModuleList.SortOrder}}</td>
                                <td [style.color]="getStatusColor(_subModuleList.Status)">
                                    <b>{{_subModuleList.Status}}</b>
                                </td>
                                <td>
                                    <i class="far fa-edit" title="Edit" (click)="Edit(_subModuleList)"></i>
                                </td>
                                <td>
                                    <i class="fas fa-lock-open" title="Open" (click)="Open(_subModuleList)"></i>
                                </td>
                                <td>
                                    <i class="fas fa-lock" title="Close" (click)="Close(_subModuleList)"></i>
                                </td>
                            <tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>

<ng-container *ngIf="EnableJson">
    <div class="col-md-3">
        <pre>{{postSubMainModule | json}}</pre>
    </div>
</ng-container>

<div class="modal fade" id="PrintDetailsTab" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document" style="max-width: 900px">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button"
                    class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary"
                    id="exampleModalLabel" (click)=print()><i class="fal fa-print"></i>
                    Print</button>
                <h3 class="modal-title">{{bcode}}-Sub Module List</h3>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="table-responsive" id="TableData">
                            <table class="table table-bordered table-hover">
                                <thead class="topbar">
                                    <tr>
                                        <th>ModuleID </th>
                                        <th>Module Name </th>
                                        <th>ModuleID </th>
                                        <th>Salesman Name</th>

                                    </tr>
                                </thead>
                                <tbody *ngFor="let _List of printData;let i=index">
                                    <tr>
                                        <td>{{_List.ModuleID}}</td>
                                        <td>{{_List.Name}}</td>
                                        <td>{{_List.ID}}</td>
                                        <td>{{_List.Name}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>