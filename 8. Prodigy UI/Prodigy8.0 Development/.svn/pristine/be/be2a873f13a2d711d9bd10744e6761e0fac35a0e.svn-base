<div class="row">
  <div class="col-md-3 float-right">
  </div>
  <div class="col-md-3 float-right">
    <h2 style="color: rgb(16, 57, 238); text-align: right; font-style: italic;"
      *ngIf="estDetails.OfferDiscountAmount>0">Discount
      Amt:{{estDetails.OfferDiscountAmount | number
      : '1.2-2'}}</h2>
  </div>
  <!-- <div class="col-md-1 float-right">
  </div> -->
  <div class="col-md-3" style="text-align: right;">
    <h3 class="font-light">Est.Date : {{applicationDate | date:'dd/MM/yyyy'}}</h3>
  </div>
  <div class="col-md-3">
    <app-lastdocno [routerUrl]="routerUrl" style="color: brown; text-align: right; font-style: italic;"></app-lastdocno>
  </div>
</div>

<div class="row">
  <div class="col-md-7">
    <label *ngFor="let radiobutton of radioItems" class="form-check-inline">
      <input type="radio" name="option" (click)="OnRadioBtnChnge(radiobutton)" class="form-check-input"
        [value]="radiobutton" [checked]="radiobutton === model.option">
      <h3 style="margin-top: 5px;">{{radiobutton}}</h3>
    </label>
  </div>
  <div class="col-md-2">
    <!-- <app-mergeestimation></app-mergeestimation> -->
  </div>
  <div class="col-md-2">
    <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary mt-2"
      (click)="loadalltheEstimation()"><i class="fas fa-sync"></i>&nbsp;Estimations</button>
  </div>
</div>


<div id="accordion" class="accordion">
  <div class="card mb-0" *ngIf="!NewEstimation">
    <div class="card-header collapsed"
      style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
      <a class="card-title align-middle Pointer " (click)="ToggleTotalFunction()" data-toggle="collapse" href="#header">
        <i class="fal fa-info-circle fa-2x align-middle mr-2"></i> <span class="align-middle"><ins>Header
            Details</ins></span>
      </a>
      <ng-container *ngIf="!ToggleTotal">
        <div class="col-md-2 pull-right">
        </div>
        <div class="col-md-2 pull-right">
        </div>
        <div class="col-md-2 pull-right">
        </div>
        <div class="col-md-2 pull-right">
          <h4><span class="align-middle">Est No: {{estDetails.EstNo}}</span></h4>
        </div>
      </ng-container>
    </div>
    <form>
      <div id="header" class="card-body show">
        <div class="row" *ngIf="ExistingEstimation">
          <div class="col-md-2">
            <div class="form-group">
              <label for="exampleAccount">Estimation No</label>
              <div class="input-group">
                <!-- <input class="form-control" id="exampleAccount" placeholder="Est No" type="text" #estNo> -->
                <input type="text" class="form-control" [(ngModel)]="estNo" (ngModelChange)="estNo=$event"
                  [ngModelOptions]="{standalone: true}"
                  oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');"
                  onKeyPress="if(this.value.length==7) return false;" placeholder="Est No" GreenBorder />

              </div>
            </div>
          </div>
          <div class="col-md-2">
            <div class="form-group">
              <label for=""></label>
              <div class="input-group">
                <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary"
                  (click)="getEstimationDetails()"><i class="fal fa-search"></i> Search</button>
              </div>
            </div>
          </div>
          <div class="col-md-2 ">
            <div class="form-group">
              <label for=""></label>
              <div class="input-group">
                <button class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
                  style="margin-top: 5px;" (click)="RecallPrint()">
                  <i class="fa fa-print"></i>&nbsp;Print</button>
              </div>
            </div>
          </div>
          <ng-container *ngIf="ApplycoinOffer">
            <div class="col-md-3">
              <div class="form-group">
                <label for=""></label>
                <div class="input-group">
                  <button class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
                    style="margin-top: 5px;" (click)="loadOfferDetails()">
                    <i class="fa fa-plus"></i>&nbsp;Apply Offer Discount</button>
                </div>
              </div>
            </div>
            <div class="col-md-3">
              <div class="form-group">
                <label for=""></label>
                <div class="input-group">
                  <button class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
                    style="margin-top: 7px;" (click)="RemoveOfferDetails()">
                    <i class="fa fa-times"></i>&nbsp;Remove Offer Discount</button>
                </div>
              </div>
            </div>
          </ng-container>
          <ng-container *ngIf="ApplyFab2020Offer">
            <div class="col-md-3">
              <div class="form-group">
                <label for=""></label>
                <div class="input-group">
                  <button class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
                    style="margin-top: 5px;" (click)="ApplyFab2022Offer()">
                    <i class="fa fa-plus"></i>&nbsp;Apply FAB-2020 Offer Discount</button>
                </div>
              </div>
            </div>
            <div class="col-md-3">
              <div class="form-group">
                <label for=""></label>
                <div class="input-group">
                  <button class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
                    style="margin-top: 7px;" (click)="RemoveFab2022Offer()">
                    <i class="fa fa-times"></i>&nbsp;Remove  Offer Discount</button>
                </div>
              </div>
            </div>
          </ng-container>
        </div>

        <div class="row" *ngIf="OrderNo">
          <div class="col-md-2">
            <div class="form-group">
              <label for="exampleAccount">Existing Order No</label>
              <div class="input-group">
                <!-- <input  type="number" class="form-control" id="exampleAccount" maxlength="7" placeholder="Order No" #orderNo> -->
                <input type="text" class="form-control"
                  oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*)\./g, '$1');"
                  onKeyPress="if(this.value.length==7) return false;" 
                  (keyup.enter)="getEstimationOrderDetails(orderNo.value)" placeholder="Order No" GreenBorder #orderNo />
              </div>
            </div>
          </div>
          <div class="col-md-3 ">
            <div class="form-group">
              <label for=""></label>
              <div class="input-group">
                <button class="btn waves-effect waves-light btn-rounded btn-outline-secondary"
                  (click)="getEstimationOrderDetails(orderNo.value)"><i class="fal fa-search"></i> Search</button>
              </div>
            </div>
          </div>
        </div>
        <ng-container *ngIf="EnableJson">
          <div class="row">
            <div class="col-sm-4">
              <pre>{{OfferModel2022 | json}}</pre>
            </div>
          </div>
        </ng-container>
      </div>
    </form>
  </div>
  <br *ngIf="!NewEstimation">

  <div id="accordian" class="accordion">
    <div class="card mb-0">
      <div class="card-header Pointer" href="#Customer" (click)="ToggleCustomerData()"
        [ngClass]="CollapseCustomerDetailsTab ? 'collapsed' : ''"
        [attr.data-toggle]="CollapseCustomerDetailsTab ? '':'collapse'"
        style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
        <div class="row">
          <div class="col-md-2 ">
            <a class="card-title align-middle Pointer" style="color:#0096FF">
              <i class="fas fa-2x fa-users mr-2 align-middle"></i>
              <span class="align-middle Pointer"><ins>Customer Details</ins></span>
            </a>
          </div>
          <!-- <ng-container *ngIf=" ToggleCustomer"> -->
          <div class="col-md-1 pull-right">
          </div>
          <div class="col-md-2 pull-right" *ngIf="EstNo > '0'" style="color:#0096FF">
            <h4><span class="align-middle"> Est No: {{EstNo}} </span></h4>
          </div>
          <ng-container *ngIf="CollapseCustomerTab">
            <div class="col-md-2 pull-right" *ngIf="CustomerName.ID != 0">
              <h4><span class="align-middle"> ID: {{CustomerName.ID}}</span></h4>
            </div>
            <div class="col-md-3 pull-right" *ngIf="CustomerName.ID != 0">
              <h4><span class="align-middle"> Customer: {{CustomerName.CustName}}</span></h4>
            </div>
          </ng-container>
          <!-- </ng-container> -->
        </div>
      </div>
      <div id="Customer" class="card-body collapse" [collapse]="CollapseCustomerDetailsTab">
        <app-customer>
        </app-customer>
      </div>
    </div>
    <br>
    <div class="card mb-0">
      <div class="card-header Pointer" href="#Sales" (click)="ToggleSalesData()"
        [ngClass]="EnableSalesTab ? 'collapsed' : ''" [attr.data-toggle]="EnableSalesTab ? '':'collapse'"
        style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
        <div class="row">
          <div class="col-md-2 ">
            <a class="card-title align-middle" style="color:#005BFF">
              <i class="fal fa-cash-register fa-2x mr-2 align-middle"></i>
              <span class="align-middle Pointer"><ins>Sales Details</ins></span>
            </a>
          </div>
          <!-- <ng-container *ngIf="ToggleSales">
            <ng-container *ngIf="NoRecordsSales"> -->
          <ng-container *ngIf="SalesData && SalesTotAmount > 0">
            <div class="col-md-1 pull-right">
              <h4><span class="align-middle"> Qty: {{SalesTotalItems}} </span></h4>
            </div>
            <div class="col-md-2 pull-right">
              <h4><span class="align-middle"> Gross. Wt: {{SalesGrossWt | number : '1.3-3'}}</span>
              </h4>
            </div>
            <div class="col-md-2 pull-right">
              <h4><span class="align-middle"> Net Wt: {{SalesNetWt | number : '1.3-3'}}</span>
              </h4>
            </div>
            <div class="col-md-3 pull-right">
              <h4><span class="align-middle"> Sales Amt:
                  {{SalesTotAmount | currencyFormat : SalesTotAmount}}</span>
              </h4>
            </div>
          </ng-container>
          <!-- </ng-container>
          </ng-container> -->
        </div>
      </div>
      <!-- <ng-container *ngIf="NoRecordsSales"> -->
      <div id="Sales" class="card-body collapse" [collapse]="EnableSalesTab">
        <!-- <app-sales (SalesAmountChange) ='SalesTotalAmountCalculation($event)' (valueChange)='SalesAmountCalculation($event)'></app-sales> -->
        <app-sales (SalesAmountChange)='SalesTotalAmountCalculation($event)'
          (SalesTaxableAmountChange)='SalesTaxableAmountCalculation($event)'
          (SalesTotalItemsChange)='SalesTotalItemsCalculation($event)'
          (SalesGrossWtChange)='SalesGrossWtCalculation($event)' (SalesNetWtChange)='SalesNetWtCalculation($event)'
          (DelCoinOffer)='DelCoinOfferCalculation($event)'>
        </app-sales>
      </div>
      <!-- </ng-container> -->
    </div>
    <br>
    <div class="card mb-0">
      <div class="card-header Pointer" href="#Purchase" (click)="TogglePurchaseData()"
        [ngClass]="EnablePurchaseTab ? 'collapsed' : ''" [attr.data-toggle]="EnablePurchaseTab ? '':'collapse'"
        style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
        <div class="row">
          <div class="col-md-2 ">
            <a class="card-title align-middle Pointer" style="color:#FF00C5">
              <i class="far fa-shopping-cart fa-2x mr-2 align-middle"></i>
              <span class="align-middle Pointer"><ins>Purchase</ins></span>
            </a>
          </div>
          <!-- <ng-container *ngIf="TogglePurchase">
            <ng-container *ngIf="NoRecordsPurchase"> -->
          <ng-container *ngIf="SalesData && PurchaseTotAmount > 0">
            <div class="col-md-1 pull-right">
            </div>
            <div class="col-md-2 pull-right">
              <h4>
                <span class="align-middle"> Gross. Wt:
                  {{PurchaseGrossWt | number : '1.3-3'}}</span>
              </h4>
            </div>
            <div class="col-md-2 pull-right">
              <h4>
                <span class="align-middle"> Net Wt: {{PurchaseNtwt | number : '1.3-3'}}</span>
              </h4>
            </div>
            <div class="col-md-3 pull-right">
              <h4><span class="align-middle"> Purchase Amt:
                  {{PurchaseTotAmount | currencyFormat : PurchaseTotAmount}}</span>
              </h4>
            </div>
          </ng-container>
          <!-- </ng-container>
          </ng-container> -->
        </div>
      </div>
      <div id="Purchase" class=" collapse" [collapse]="EnablePurchaseTab" style="padding:10px;">
        <!-- <app-purchase (PurchaseAmountChange) ='PurchaseTotalAmountCalculation($event)' (valueChange)='PurchaseAmountCalculation($event)'></app-purchase> -->
        <app-purchase (PurchaseAmountChange)='PurchaseTotalAmountCalculation($event)'
          (PurchaseNtWt)='PurchaseNtWtCalculation($event)' (PurchaseGrossWt)='PurchaseGrossWtCalculation($event)'>
        </app-purchase>
      </div>
    </div>
    <br>

    <div class="card mb-0">
      <div class="card-header Pointer" href="#Orders" (click)="ToggleOrderAttachment()"
        [ngClass]="EnableOrderAttachmentTab ? 'collapsed' : ''"
        [attr.data-toggle]="EnableOrderAttachmentTab ? '':'collapse'"
        style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
        <div class="row">
          <div class="col-md-2 ">
            <a class="card-title align-middle Pointer" style="color:#00BBD6" data-toggle="collapse" href="#Orders">
              <i class="fal fa-file-invoice fa-2x mr-2 align-middle"></i>
              <span class="align-middle Pointer"><ins>Orders</ins></span>
            </a>
          </div>
          <ng-container *ngIf="SalesData && OrderTotAmount > 0">
            <div class="col-md-1 pull-right">
            </div>
            <div class="col-md-2 pull-right">
            </div>
            <div class="col-md-2 pull-right">
            </div>
            <div class="col-md-3 pull-right">
              <h4><span class="align-middle"> Order Amount:
                  {{OrderTotAmount | number : '1.2-2'}}</span>
              </h4>
            </div>
          </ng-container>
        </div>
      </div>
      <div id="Orders" class="card-body collapse" [collapse]="EnableOrderAttachmentTab">
        <!-- <app-attachorder (OrderAmountChange) ='OrderTotalAmountCalculation($event)' (valueChange)='OrderAmountCalculation($event)'></app-attachorder> -->
        <app-attachorder (OrderAmountChange)='OrderTotalAmountCalculation($event)'></app-attachorder>
      </div>
    </div>
    <br>

    <div class="card mb-0">
      <div class="card-header Pointer" href="#SalesReturn" (click)="ToggleSRAttachment()"
        [ngClass]="EnableSRAttachmentTab ? 'collapsed' : ''" [attr.data-toggle]="EnableSRAttachmentTab ? '':'collapse'"
        style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
        <div class="row">
          <div class="col-md-2 ">
            <a class="card-title align-middle Pointer" style="color: #3F51B5;" href="#SalesReturn"
              data-toggle="collapse">
              <i class="fas fa-exchange-alt fa-2x mr-2 align-middle"></i>
              <span class="align-middle Pointer"><ins>SR Est</ins></span>
            </a>
          </div>
          <ng-container *ngIf="SalesData && SRTotAmount > 0">
            <div class="col-md-1 pull-right">
            </div>
            <div class="col-md-2 pull-right">
            </div>
            <div class="col-md-2 pull-right">
            </div>
            <div class="col-md-3 pull-right">
              <h4><span class="align-middle"> SR. Amt:
                  {{SRTotAmount | currencyFormat : SRTotAmount}}</span>
              </h4>
            </div>
          </ng-container>
        </div>
      </div>
      <div id="SalesReturn" class="card-body collapse" [collapse]="EnableSRAttachmentTab">
        <app-attachsalesreturn (SRAmountChange)='SRTotalAmountCalculation($event)'></app-attachsalesreturn>
      </div>
    </div>
    <br>

    <div class="card mb-0">
      <div class="card-header Pointer" href="#OldGold" (click)="ToggleOldGoldAttachment()"
        [ngClass]="EnableOldGoldAttachmentTab ? 'collapsed' : ''"
        [attr.data-toggle]="EnableOldGoldAttachmentTab ? '':'collapse'"
        style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
        <div class="row">
          <div class="col-md-2 ">
            <a class="card-title align-middle" style="color:#FF00C5">
              <i class="far fa-rupee-sign fa-2x mr-2 align-middle"></i>
              <span class="align-middle Pointer"><ins>Old Gold Est.</ins></span>
            </a>
          </div>
          <ng-container *ngIf="SalesData && OGTotAmount > 0">
            <div class="col-md-1 pull-right">
            </div>
            <div class="col-md-2 pull-right">
            </div>
            <div class="col-md-2 pull-right">
            </div>
            <div class="col-md-3 pull-right">
              <h4><span class="align-middle"> Old Gold Amount:
                  {{OGTotAmount | currencyFormat : OGTotAmount}}</span>
              </h4>
            </div>
          </ng-container>
        </div>
      </div>
      <div id="OldGold" class="card-body collapse" [collapse]="EnableOldGoldAttachmentTab">
        <!-- <app-attacholdgold (OGAmountChange) ='OGTotalAmountCalculation($event)' (valueChange)='OldGoldAmountCalculation($event)'></app-attacholdgold> -->
        <app-attacholdgold (OGAmountChange)='OGTotalAmountCalculation($event)'></app-attacholdgold>
      </div>
    </div>

    <br>
    <ng-container *ngIf="EnableOfferAndCorpScreen">
      <div class="card mb-0">
        <div class="card-header Pointer" href="#WeightScheme" (click)="ToggleWeightScheme()"
          [ngClass]="EnableWeightScheme ? 'collapsed' : ''" [attr.data-toggle]="EnableWeightScheme ? '':'collapse'"
          style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
          <div class="row">
            <div class="col-md-2 ">
              <a class="card-title align-middle" style="color:#1e3dee">
                <i class="far fa-rupee-sign fa-2x mr-2 align-middle"></i>
                <span class="align-middle Pointer"><ins>Weight Scheme</ins></span>
              </a>
            </div>
            <ng-container>
              <div class="col-md-1 pull-right">
              </div>
              <div class="col-md-2 pull-right">
              </div>
              <div class="col-md-2 pull-right">
              </div>

            </ng-container>
          </div>
        </div>
        <div id="WeightScheme" class="card-body collapse" [collapse]="EnableWeightScheme">
          <div class="row">
            <button class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
              style="margin-top: 5px;" (click)="OpenAtachWeightSchemeModel()">
              <i class="fa fa-plus"></i>&nbsp;Attach Weight Scheme</button>
          </div>
          <br>
          <div class="row">
            <div class="table-responsive">
              <table class="table table-bordered">
                <thead class="topbar">
                  <tr>
                    <th>Node</th>
                    <th>Amount</th>
                    <th>Ref.No</th>
                    <th>Details</th>
                    <th>Ap.no/Gr</th>
                    <th>Branch</th>
                    <th>Delete</th>
                  </tr>
                </thead>
