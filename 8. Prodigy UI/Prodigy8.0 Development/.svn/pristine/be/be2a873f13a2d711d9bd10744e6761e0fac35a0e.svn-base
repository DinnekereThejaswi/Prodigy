      <div class="card mb-0">
        <div class="card-header Pointer" href="#SchemeAdjust" (click)="ToggleSchemeAdjust()"
          [ngClass]="EnableSchemeAdjust ? 'collapsed' : ''" [attr.data-toggle]="EnableSchemeAdjust ? '':'collapse'"
          style="background-color: #fdfdfd; box-shadow: -1px 0px 2px 0px rgba(0, 0, 0, 0.36);color:black">
          <div class="row">
            <div class="col-md-2 ">
              <a class="card-title align-middle" style="color:#f81109">
                <i class="far fa-rupee-sign fa-2x mr-2 align-middle"></i>
                <span class="align-middle Pointer"><ins> Scheme Adjust</ins></span>
              </a>
            </div>
            <ng-container>
              <div class="col-md-2 pull-right">
              </div>
              <div class="col-md-3 pull-right">
                <b>Scheme Amount :*****</b>
              </div>
              <div class="col-md-2 pull-right">
              </div>
              <div class="col-md-2 pull-right">
              </div>

            </ng-container>
          </div>
        </div>
        <div id="WeightScAdjustheme" class="card-body collapse" [collapse]="EnableSchemeAdjust">
          <div class="row">
            <button class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary "
