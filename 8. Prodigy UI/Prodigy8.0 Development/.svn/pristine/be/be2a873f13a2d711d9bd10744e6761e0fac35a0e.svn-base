                <div class="col-md-4">
                  <div class="form-group">
                    <label class="control-label" for="EndMSNo">End MSNo</label>
                    <select class="form-control" formControlName="frmCtrl_EndMSNo" [(ngModel)]="SchemeAdjust.EndMSNo">
                      <option [ngValue]="null">select</option>
                      <option></option>
                    </select>
                  </div>
                </div>
                <div class="col-md-4">
                  <div class="form-group">
                    <label class="control-label" for="Discount">Discount Amt</label>
                    <input type="text" class="form-control" formControlName="frmCtrl_DiscountAmt"
                      [(ngModel)]="SchemeAdjust.DiscountAmt" readonly disabled placeholder="0.00">
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-md-4">
                  <br>
                  <div class="form-group">
                    <button type="submit" class="btn btn-success waves-effect waves-light btn-rounded btn-secondary"><i
                        class="fal fa-save"></i>&nbsp; Add</button>
                    <button type="submit"
                      class="btn btn-danger  waves-effect waves-light btn-rounded btn-secondary">&nbsp;Cancel</button>
                  </div>
                </div>

              </div>
            </form>
            <ng-container *ngIf="EnableJson">
              <pre>{{ SchemeAdjust | json}}</pre>
            </ng-container>
          </div>
        </div>
      </div>
    </div>
  </ng-container>