<div class="container emp-profile">
    <div class="row">
        <div class="col-md-3">
            <div class="profile-img">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRrpXZJYwArxfNRshSFcJSHsVRKFr4i_MOadvYRDcfQBFkjhQxO"
                    class="m-x-auto img-fluid img-circle" alt="avatar">
            </div>
            <br>
            <div class="form-group" style="text-align: center">
                <b style="font-size: 30px">
                    {{CustomerListData.CustName}}
                </b>
                <p>{{CustomerListData.City}}</p>

            </div>

        </div>
        <div class="col-md-9">
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Customer ID</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.ID}} readonly>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="form-group">
                        <label>Mobile</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.MobileNo}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Email</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.EmailID}} readonly>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Address 1</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.Address1}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Address 2</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.Address2}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Address 3</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.Address3}} readonly>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label>City</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.City}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Pincode</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.Pincode}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>PAN</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.PANNo}} readonly>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-4">
                    <div class="form-group">
                        <label>ID Type</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.IDType}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>ID Details</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.IDDetails}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Date Of Birth</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.DateOfBirth|date}}
                            readonly>
                    </div>
                </div>
            </div>
            <div class="row">

                <div class="col-md-4">
                    <div class="form-group">
                        <label>GSTIN</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.TIN}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>Phone No</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.PhoneNo}} readonly>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="form-group">
                        <label>State</label>
                        <input type="text" class="form-control" placeholder={{CustomerListData.State}} readonly>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>