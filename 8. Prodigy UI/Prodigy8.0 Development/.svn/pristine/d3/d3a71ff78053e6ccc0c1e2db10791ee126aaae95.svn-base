<div class="card">
    <div class="card-body">
        <form [formGroup]="ReprintBarcodeForm" class="form-horizontal" autocomplete="off">
            <div class="row">
                <div class="col-auto">
                    <label for="Barcode1" class="col-form-label"><strong>Barcode No : 1 </strong></label>
                </div>
                <div class="col-sm-3">
                    <input class="form-control" type="text" formControlName="Barcode1"
                        [(ngModel)]="ReprintBarcodeHeaderModel.Barcode1" (keyup.enter)="FetchBarcodeNo1(Barcode1.value)"
                        placeholder="Please Enter Barcode no 1" #Barcode1 id="Barcode1" maxlength="10"
                        onkeypress='return (event.charCode > 96 && event.charCode < 123) || (event.charCode > 64 && event.charCode < 91) || (event.charCode >= 48 && event.charCode <= 57) || event.charCode <= 31'>
                </div>
                <div class="col-auto">
                    <label for="Barcode2" class="col-form-label"> <strong>Barcode No : 2 </strong></label>
                </div>
                <div class="col-sm-3">
                    <input class="form-control" type="text" formControlName="Barcode2"
                        [(ngModel)]="ReprintBarcodeHeaderModel.Barcode2" (keyup.enter)="FetchBarcodeNo2()"
                        placeholder="Please Enter Barcode no 2" #Barcode2 id="Barcode2" maxlength="10"
                        onkeypress='return (event.charCode > 96 && event.charCode < 123) || (event.charCode > 64 && event.charCode < 91) || (event.charCode >= 48 && event.charCode <= 57) || event.charCode <= 31'>
                </div>
            </div>
            <br>
        </form>
        <div class="row">
            <div class="col-sm-1"></div>
            <div class="col-sm-6">
                <div class=" my-6">
                    <div class="col-md-12 text-center">
                        <button id="single" class="btn  btn-success waves-effect waves-light btn-box btn-secondary MT-2"
                            (click)="left()"><i class="fa fa-arrow-left"></i>
                            Left</button>
                        <button id="single" class="btn btn-success waves-effect waves-light btn-box btn-secondary"
                            (click)="right()"><i class="fa fa-arrow-right"></i>
                            Right</button>
                        <button id="double" class="btn btn-success waves-effect waves-light btn-box btn-secondary"
                            (click)="double()"><i class="fa fa-exchange"></i>
                            Double</button>
                        <button class="btn  btn-warning  waves-effect waves-light btn-box btn-secondary MT-2"
                            (click)="clear()"><i class="fal fa-window-close"></i>
                            Clear</button>
                    </div>
                </div>
            </div>
            <div class="col-sm-3">

            </div>
        </div>


        <br>

    </div>
</div>

<ng-container *ngIf="EnableJson">
    <div class="col-md-3">
        <pre>{{ ReprintBarcodeHeader | json }}</pre>
        <pre>{{ ReprintBarcodeHeaderModel | json }}</pre>
    </div>
</ng-container>

