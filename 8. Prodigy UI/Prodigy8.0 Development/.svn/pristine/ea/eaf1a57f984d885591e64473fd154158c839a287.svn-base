  <div class="row" style="display:block;margin: auto;width: 30%;">
    <div class="modal fade" id="RepairDeliveryDirectModal" tabindex="-1" role="dialog"
      aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog modal-lg" role="document" style="max-width: 1400px">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button"
              class="btn btn-outline-success waves-effect waves-light btn-rounded btn-outline-secondary"
              id="exampleModalLabel" (click)=printRepairDelivery()><i class="fal fa-print"></i> Print</button>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
              <span aria-hidden="true">&times;</span>
            </button>
          </div>
          <ng-container *ngIf="PrintTypeBasedOnConfig">
            <div class="container invoice-box htmlPrint" id="print-closed-section"
              style="max-width: 900px;border-width: 2em" *ngIf="PrintTypeBasedOnConfig.PrintType == 'RAW'">
              <pre>{{DeliveryNoDetails}}</pre>
            </div>
            <span *ngIf="PrintTypeBasedOnConfig.PrintType == 'HTML'">
              <div id="DisplayRepairDeliveryData" class="center" [innerHTML]='DeliveryNoDetails | safeHtml'></div>
            </span>
          </ng-container>
        </div>
      </div>
    </div>
  </div>
  <!-- </ng-container> -->
</div>


<div class="modal fade" id="ReprintTypeModal" tabindex="-1" role="dialog" aria-labelledby="ReprintTypeTabLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="container invoice-box htmlPrint" style="max-width: 500px;border-width: 2em">
        <div class="invoice">
          <h3 style="text-align:center"><b>Reprint Type</b></h3>
          <div class="card">
            <div class="card-body">
              <div class="form-group">
                <div class="col-md-12">
                  <label *ngFor="let radiobuttonReprintType of radioReprintTypeItems" class="form-check-inline">
                    <input type="radio" name="option" (click)="ChangedReprintType(radiobuttonReprintType)"
                      class="form-check-input" [value]="radiobuttonReprintType"
                      [checked]="radiobuttonReprintType === modelReprintType.option">
                    <h3 style="margin-top: 10px;">{{radiobuttonReprintType}}</h3>
                  </label>
                </div>
              </div>
              <div class="form-group" style="text-align: center;">
                <button class="printMe btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                  (click)="onReprint(RepaireReprint)"><i class="fal fa-print"></i>
                  Print</button>
                <button class="printMe btn btn-danger waves-effect waves-light btn-rounded btn-secondary"
                  (click)="CanelPrint()"><i class="fal fa-times"></i>
                  Cancel</button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

