<div class="row" style="display:block;margin: auto;width: 30%;">
  <ng-container *ngIf="EnableHeader">
    <form [formGroup]="RepaireReprint" class="form-horizontal">
      <div class="col-md-12">
        <div class="card">
          <div class="card-body">
            <div class="row">
              <div class="col-md-12">

                <div class="form-group">
                  <div class="input-group" *ngFor="let radiobutton of radioItems">
                    <ul class="list-group">
                      <div class="col-md-12">
                        <label class="form-check-inline">
                          <input type="radio" name="option" class="form-check-input" [value]="radiobutton"
                            (change)="Changed(radiobutton)" [checked]="radiobutton === model.option">
                          <h3 style="margin-top: 5px;">{{radiobutton}}</h3>
                        </label>
                      </div>
                    </ul>
                  </div>
                </div>

              </div>
            </div>
            <div class="row" *ngIf="model.option == 'Delivery No'">
              <div class="col-md-12 ">
                <div class="form-group">
                  <label for="">Repair Delivery Number</label>
                  <input class="form-control" name="Delivery" type="number" formControlName="DeliveryNo"
                    [(ngModel)]="repairDeliverySummaryHeader.RepairNo" placeholder="Repair Delivery Number">
                </div>
              </div>
            </div>
            <div class="row" *ngIf="model.option == 'Receipt No'">
              <div class="col-md-12 ">
                <div class="form-group">
                  <label for="">Repair Receipt Number</label>
                  <input class="form-control" formControlName="ReceiptNo" type="number"
                    [(ngModel)]="repairDeliverySummaryHeader.ReceiptNo" placeholder="Repair Receipt Number">
                </div>
              </div>
            </div>
            <div class="row" *ngIf="model.option == 'Delivery No'">
              <div class="col-md-12">
                <div class="form-group">
                  <div class="form-group">
                    <input type="checkbox" class="chkbxsize" [(ngModel)]="isChecked" (change)="FieldsChange($event)" />
                    Direct
                  </div>
                </div>
              </div>
            </div>
            <div class="row">
              <div class="col-md-12">
                <div class="form-group">
                  <button class="printMe btn btn-success waves-effect waves-light btn-rounded btn-secondary"
                    (click)="onPrint(RepaireReprint)"><i class="fal fa-print"></i>
                    Reprint </button>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </form>
  </ng-container>
