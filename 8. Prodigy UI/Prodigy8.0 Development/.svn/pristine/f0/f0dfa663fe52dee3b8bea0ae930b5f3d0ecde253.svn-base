<div class="row">
  <div class="col-md-12">
    <div class="card">
      <div class="card-body">
        <form [formGroup]="DesignMasterForm" class="form-horizontal" autocomplete="off">
          <div class="row">
            <div class="col-md-3">
              <div class="form-group">
                <label class="control-label" for="code">Master Design
                  <span class="textRed"> <i class="fas fa-star-of-life"></i></span>
                </label>
                <select class="form-control" formControlName="MasterDesign"
                  [(ngModel)]="DesignMasterListData.MasterDesignCode">
                  <!-- <option [ngValue]="null">Select</option> -->
                  <option [ngValue]="null">
                    NA
                  </option>
                </select>
              </div>
            </div>
            <div class="col-md-3">
              <div class="form-group">
                <label class="control-label" for="Code">Code <span class="textRed"> <i
                      class="fas fa-star-of-life"></i></span>
                  <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                    <span class="tooltiptext tooltip-top">Design Code</span>
                  </div>
                </label>
                <input type="text" class="form-control" id="Code" [(ngModel)]="DesignMasterListData.DesignCode"
                  formControlName="Code" AlphaNumericValidator maxlength="5" placeholder="Please Enter Design Code" />
              </div>
            </div>
            <div class="col-md-3">
              <div class="form-group">
                <label class="control-label" for="Name">Name <span class="textRed"> <i
                      class="fas fa-star-of-life"></i></span>
                  <div class="tooltipdesign"> <i class="fal fa-question-circle"></i>
                    <span class="tooltiptext tooltip-top">Design Name</span>
                  </div>
                </label>
                <input type="text" class="form-control" id="Name" [(ngModel)]="DesignMasterListData.DesignName"
                  formControlName="Name" AlphaNumericValidator placeholder="Please Enter Design Name" />
              </div>
            </div>
            <div class="col-sm-3">
              <ng-container *ngIf="EnableAdd">
                <button class="btn btn-success waves-effect waves-light btn-rounded btn-secondary MT-2"
                  style="margin-top: 1.2rem !important;" (click)="Add(DesignMasterForm)">
                  <i class="far fa-plus-circle"></i>&nbsp;Add</button>
              </ng-container>
              <ng-container *ngIf="EnableSave">
                <button class="btn btn-success waves-effect waves-light btn-rounded btn-secondary MT-2"
                  style="margin-top: 1.2rem !important;" (click)="save(DesignMasterForm)">
                  <i class="fal fa-save"></i>&nbsp;Save</button>
              </ng-container>
              <button class="btn  btn-warning waves-effect waves-light btn-rounded btn-secondary MT-2"
                style="margin-top: 1.2rem !important;" (click)="clear()">
                <i class="fal fa-trash"></i>&nbsp;Clear</button>

            </div>
          </div>
        </form>
        <ng-container *ngIf="EnableJson">
          <pre> {{DesignMasterListData |json }}</pre>
        </ng-container>

      </div>
    </div>
  </div>
  <div class="col-md-12">
    <div class="card">
      <div class="card-body">
        <div class="table-responsive">
          <table class="table table-bordered">
            <thead class="topbar">
              <tr>
                <th>Master Design</th>
                <th>Salesman Code</th>
                <th>Salesman Name</th>
                <th>Status</th>
                <th>Edit</th>
                <th>Open</th>
                <th>Close</th>
              </tr>
            </thead>
            <tbody *ngFor="let _list of DesignList">
              <tr>
                <td>{{_list.MasterDesignCode}}</td>
                <td>{{_list.DesignCode}}</td>
                <td>{{_list.DesignName}}</td>
                <td [style.color]="getStatusColor(_list.ObjectStatus)"><b>{{_list.ObjectStatus}}</b></td>
                <td><i class="fas fa-edit" title="Edit" (click)="edit(_list)"></i></td>
                <td><i class="fas fa-lock-open" title="Open" (click)="openField(_list)"></i></td>
                <td><i class="fas fa-lock-alt" title="Close" (click)="closeField(_list)"></i></td>
              </tr>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>

  <!-- <pre>{{ DesignMasterListData | json }}</pre> -->
</div>